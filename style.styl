body
  width 1000px
  max-width 100%
  margin auto

  &.drag::before
    content ''
    position fixed
    top 0
    left 0
    bottom 0
    right 0
    background rgba(248,40,59,0.3)
    border 5px #f8283b dashed
    z-index 999

#map
  width 100%
  height 300px

ul
  padding-left 26px
  list-style square
